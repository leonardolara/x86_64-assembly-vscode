{
    "name": "language-x86-64-assembly",
    "displayName": "x86 and x86_64 Assembly",
    "description": "NASM x86 and x86_64 assembly syntax highlighting",
    "version": "0.0.1",
    "preview": false,
    "icon": "logo.png",
    "engines": {
        "vscode": "^1.0.0"
    },
    "author": {
        "name": "Leonardo Lara"
    },
    "publisher": "UnionSoft",
    "contributes": {
        "languages": [
            {
                "id": "asm-intel-x86-generic",
                "aliases": [
                    "x86 and x86_64 Assembly",
                    "asm-intel-x86-generic"
                ],
                "extensions": [
                    ".asm",
                    ".nasm",
                    ".yasm",
                    ".inc",
                    ".s"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "asm-intel-x86-generic",
                "scopeName": "source.asm.x86_64",
                "path": "./syntaxes/language-x86_64-assembly.tmLanguage"
            }
        ]
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "url": "https://github.com/leonardolara/x86_64-assembly-vscode",
        "type": "git"
    }
}
